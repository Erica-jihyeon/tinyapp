<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if (userID) { %>
      <p style="border: 0px; margin: 4px; padding: 4px 5px 7px; text-align: left;">Logged In As: <%= users[userID].email %>!</p>
      <form method="POST" action="/logout">
        <button type="submit" class="btn btn-outline-dark" style="margin: 4px 4px; text-align: center;">Logout</button>
      </form>
    <% } else { %>
        <a style="margin: 15px; color:lightgoldenrodyellow; font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;" href="/login">Login</a>
        <a style="margin: 10px; color:lightgoldenrodyellow; font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;" href="/register">Register</a>
    
      <!-- <form method="GET" action="/register">
        <button type="submit" class="btn btn-outline-dark" style="margin: 4px 4px; text-align: center;">Register</button>
      </form>
      <form method="GET" action="/login">
        <button type="submit" class="btn btn-outline-dark" style="margin: 4px 4px; text-align: center;">Login</button>
      </form> -->
  <% } %>
  </nav>
</header>